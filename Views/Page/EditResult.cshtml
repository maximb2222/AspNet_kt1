@model YourProjectName.Models.MessageModel

@{
    ViewData["Title"] = "Message Submitted";
}

<h1>Your Message</h1>

<div class="alert alert-success">
    <p>You submitted the following message:</p>
    <p><strong>@Model.Message</strong></p>
</div>

<a asp-action="Edit" class="btn btn-primary">Submit another message</a>